CREATE TABLE DEPTNAME(
	DEPARTMENT CHAR(10) CHECK (DEPARTMENT IN ('정형외과', '피부과', '이비인후과', '산부인과', '소아과')),
    PRIMARY KEY(DEPARTMENT)
);


INSERT INTO DEPTNAME VALUES ('정형외과');
